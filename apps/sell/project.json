{
  "name": "sell",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "app",
  "sourceRoot": "apps/sell/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "defaultConfiguration": "production",
      "outputs": ["{options.outputPath}"],
      "options": {
        "configFile": "apps/sell/vite.config.mts",
        "outputPath": "dist/apps/sell",
        "tsConfig": "apps/sell/tsconfig.app.json"
      },
      "configurations": {
        "development": {
          "mode": "development",
          "sourcemap": true
        },
        "production": {
          "sourcemap": false,
          "mode": "production"
        }
      }
    },
    "serve": {
      "executor": "@nx/vite:dev-server",
      "options": {
        "buildTarget": "sell:build",
        "port": 4200
      },
      "configurations": {
        "production": {
          "buildTarget": "sell:build:production"
        },
        "development": {
          "buildTarget": "sell:build:development",
          "hmr": true
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "buildTarget": "sell:build"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "eslintConfig": "apps/sell/eslint.config.cjs"
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["{options.reportsDirectory}"],
      "options": {
        "reportsDirectory": "../../coverage/apps/sell"
      }
    },
    "serve-static": {
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "sell:build",
        "port": 4200,
        "staticFilePath": "dist/apps/sell/browser",
        "spa": true
      }
    },
    "storybook": {
      "executor": "@storybook/angular:start-storybook",
      "options": {
        "port": 4400,
        "configDir": "apps/sell/.storybook",
        "browserTarget": "sell:build",
        "compodoc": false
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "build-storybook": {
      "executor": "@storybook/angular:build-storybook",
      "outputs": ["{options.outputDir}"],
      "options": {
        "outputDir": "dist/storybook/sell",
        "configDir": "apps/sell/.storybook",
        "browserTarget": "sell:build",
        "compodoc": false
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "test-storybook": {
      "executor": "nx:run-commands",
      "options": {
        "command": "test-storybook -c apps/sell/.storybook --url=http://localhost:4400"
      }
    },
    "static-storybook": {
      "executor": "@nx/web:file-server",
      "dependsOn": ["build-storybook"],
      "options": {
        "buildTarget": "sell:build-storybook",
        "staticFilePath": "dist/storybook/sell",
        "spa": true
      },
      "configurations": {
        "ci": {
          "buildTarget": "sell:build-storybook:ci"
        }
      }
    }
  }
}
